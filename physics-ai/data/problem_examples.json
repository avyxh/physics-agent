    "free_fall": [
      {
        "problem": "A ball is dropped from 20m height. What is its final velocity?",
        "solution": "19.8 m/s",
        "method": "kinematics",
        "steps": [
          "Given: h = 20m, u = 0 m/s, g = 9.81 m/s²",
          "Use: v² = u² + 2gh",
          "v² = 0² + 2(9.81)(20) = 392.4",
          "v = √392.4 = 19.8 m/s"
        ]
      },
      {
        "problem": "An object falls for 3 seconds. How far does it travel?",
        "solution": "44.1 m",
        "method": "kinematics",
        "steps": [
          "Given: t = 3s, u = 0 m/s, g = 9.81 m/s²",
          "Use: s = ut + 0.5gt²",
          "s = 0(3) + 0.5(9.81)(3²) = 44.1 m"
        ]
      }
    ],
    "projectile_motion": [
      {
        "problem": "A ball is thrown at 45° with velocity 20 m/s. Find the range.",
        "solution": "40.8 m",
        "method": "kinematics",
        "steps": [
          "Given: v₀ = 20 m/s, θ = 45°, g = 9.81 m/s²",
          "Range formula: R = v₀²sin(2θ)/g",
          "R = (20²)sin(90°)/9.81 = 400/9.81 = 40.8 m"
        ]
      }
