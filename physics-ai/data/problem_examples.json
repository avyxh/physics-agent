    "pendulum": [
      {
        "problem": "A pendulum has length 1.5m. What is its period?",
        "solution": "2.45 s",
        "method": "oscillations",
        "steps": [
          "Given: L = 1.5m, g = 9.81 m/s²",
          "Period formula: T = 2π√(L/g)",
          "T = 2π√(1.5/9.81) = 2π√0.153 = 2.45 s"
        ]
      }
    ],
    "collision": [
      {
        "problem": "Ball A (2kg, 5m/s) hits stationary Ball B (3kg). Find final velocities for elastic collision.",
        "solution": "vₐ = -1.0 m/s, vᵦ = 4.0 m/s",
        "method": "momentum_conservation",
        "steps": [
          "Given: mₐ = 2kg, uₐ = 5m/s, mᵦ = 3kg, uᵦ = 0m/s",
          "Elastic collision formulas:",
          "vₐ = ((mₐ-mᵦ)uₐ + 2mᵦuᵦ)/(mₐ+mᵦ) = (2-3)(5)/(2+3) = -1.0 m/s",
          "vᵦ = ((mᵦ-mₐ)uᵦ + 2mₐuₐ)/(mₐ+mᵦ) = 2(2)(5)/(2+3) = 4.0 m/s"
        ]
      }
    ]
